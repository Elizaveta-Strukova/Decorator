<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="546.0" prefWidth="526.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.decorator.HelloController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <AnchorPane prefHeight="445.0" prefWidth="486.0">
         <children>
            <Pane fx:id="paneLight" prefHeight="200.0" prefWidth="200.0" />
            <Pane fx:id="panePresent" prefHeight="200.0" prefWidth="200.0" />
            <Pane fx:id="paneTree" prefHeight="200.0" prefWidth="200.0" />
            <Pane fx:id="paneStar" prefHeight="200.0" prefWidth="200.0" />
            <Label layoutX="167.0" layoutY="14.0" prefHeight="28.0" prefWidth="180.0" text="Новогодняя ёлка!" textFill="RED">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <HBox prefHeight="74.0" prefWidth="489.0">
         <children>
            <AnchorPane prefHeight="27.0" prefWidth="480.0">
               <children>
                  <Button layoutX="397.0" layoutY="24.0" mnemonicParsing="false" onAction="#del" text="Удалить" />

                  <Button layoutX="1.0" layoutY="24.0" mnemonicParsing="false" onAction="#addStar" prefHeight="3.0" prefWidth="76.0" text="Звезда" />
                  <Button layoutX="98.0" layoutY="24.0" mnemonicParsing="false" onAction="#addPresents" text="Подарки" />
                  <Button layoutX="191.0" layoutY="24.0" mnemonicParsing="false" onAction="#addLights" text="Гирлянда" />
                  <Button layoutX="285.0" layoutY="24.0" mnemonicParsing="false" onAction="#addAll" text="Добавить все" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </children>


</VBox>
